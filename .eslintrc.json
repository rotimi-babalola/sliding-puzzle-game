{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "jest": true,
        "es6": true
    },
    "rules": {
        "arrow-parens": "off",
        "object-curly-newline": "off",
        "no-underscore-dangle": "off",
        "import/prefer-default-export": "off",
        "react/destructuring-assignment": "off",
        "no-plusplus": "off",
        "react/jsx-filename-extension": "off",
        "implicit-arrow-linebreak": "off"
    }
}
